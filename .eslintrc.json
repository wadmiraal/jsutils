{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": "off",
    "space-before-function-paren": "off",
    "quotes": "off",
    "comma-dangle": "off"
  }
}
